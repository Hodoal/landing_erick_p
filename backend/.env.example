# Variables de entorno - Backend

# Puerto del servidor
PORT=3000

# URL del frontend
FRONTEND_URL=http://localhost:5173

# Google Calendar API
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_REDIRECT_URI=http://localhost:3000/auth/google/callback
GOOGLE_CALENDAR_ID=primary

# Nodemailer - Configuración de email
EMAIL_HOST=smtp.gmail.com
EMAIL_PORT=587
EMAIL_SECURE=false
EMAIL_USER=tu-email@gmail.com
EMAIL_PASSWORD=tu-app-password

# Email del organizador
ORGANIZER_EMAIL=organizador@ejemplo.com
ORGANIZER_NAME=Erick Ads

# Ruta para guardar archivos Excel
EXCEL_EXPORT_PATH=./exports

# Configuración de la reunión
MEETING_DURATION=75
MEETING_TIMEZONE=America/Bogota
